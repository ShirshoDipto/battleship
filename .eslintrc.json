{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
    "no-unused-vars": "warn",
    "no-param-reassign": "off",
    "no-alert": "off",
    "radix": "off",
    "no-await-in-loop": "off"
  }
}
